<?xml version="1.0" encoding="utf-8"?><!--<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="100">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="5"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:gravity="center"
            android:text="1234 x 3 - 2468"
            android:textSize="24dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="17"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:gravity="center"
            android:text="1.234"
            android:textSize="80dp"
            android:textStyle="bold" />
    </LinearLayout>

    <include
        layout="@layout/calc_memory"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="8" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="70"
        android:columnCount="4"
        android:rowCount="5">

        <LinearLayout
            android:id="@+id/standard_btn_percentage"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="%" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_sqrt"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="sqrt" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_sq"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="x^2" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_oneTile"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="1/x" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_CE"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="CE" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_C"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="C" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_del"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="DEL" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_divide"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="/" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_7"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="7"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_8"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="8"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_9"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="9"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_times"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="x" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_4"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="4"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_5"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="5"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_6"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="6"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_subtract"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="-" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_1"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="1"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_2"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="2"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_3"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="3"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_add"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="+" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_minusplus"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="+-" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_0"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="0"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_point"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="," />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/standard_btn_calc"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:background="#45494A"
            android:orientation="vertical"
            android:theme="@style/AppTheme.standard_Buttons">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="=" />
        </LinearLayout>
    </GridLayout>
</LinearLayout><!--</android.support.constraint.ConstraintLayout>-->